.all {
	margin-top: 20rpx;
}
.all .bar {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 30rpx 20rpx;
	padding: 10rpx;
}
.bar .barbtn {
	height: 60rpx;
	line-height: 60rpx;
	font-size: 24rpx;
}
.all .week {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	/* padding: 20upx;
	padding-left: 40upx;
	padding-right: 40upx; */
	/* border:1px solid red; */
	/* background-color: #fff; */
	padding: 20rpx 36rpx 20rpx 20rpx;
	border-radius: 20rpx;
	color:#fff;
	font-size:30rpx;
}
.myDateTable {
	margin: 20rpx;
	/* padding: 20upx 0upx 20upx 18upx; */
	border-radius: 20rpx;
	background: -webkit-linear-gradient(#74AADA, #94db98);
	background: linear-gradient(#74AADA, #94db98);
}
.myDateTable .dateCell {
	/* width: 11vw; */
	/* width: 22upx; */
	width:65rpx;
	/* padding: 1vw; */
	display: inline-block;
	text-align: center;
	font-size: 16px;
	margin:19rpx;
}
.dateCell .cell {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	border-radius: 50%;
	height: 65rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
